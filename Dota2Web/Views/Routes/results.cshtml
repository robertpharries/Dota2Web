<form ng-submit="search()">
    <label for="accid">Account ID:</label>
    <input type="text" ng-model="searchForm.accountId" id="accid" />

    <button type="submit">Get Matches</button>
</form>
<h2>Results</h2>
<h3>@ViewBag.id</h3>
<table>
    <tr>
        <th>Match Id</th>
        <th>Result</th>
    </tr>
    @foreach (var md in ViewBag.mdList)
    {
        <tr>
            <td><a href="https://www.dotabuff.com/matches/@md.getMatchId()">@md.getMatchId()</a></td>
            @if(md.playerResult(ViewBag.id))
            {
                <td>Won</td>
            } else
            {
                <td>Lost</td>
            }
        </tr>
    }
</table>